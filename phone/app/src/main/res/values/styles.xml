<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <!--<item name="colorPrimary">@color/colorPrimary</item>-->
        <!--<item name="colorPrimaryDark">@color/colorPrimaryDark</item>-->
        <!--<item name="colorAccent">@color/colorAccent</item>-->
    </style>

    <style name="AppFullscreen" parent="AppTheme">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    
    <style name="LoginLayoutButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/common_button_height</item>
        <item name="android:background">@drawable/shape_common_button_bg_disable</item>
        <item name="android:layout_marginLeft">@dimen/login_edit_margin_l_and_r</item>
        <item name="android:layout_marginStart">@dimen/login_edit_margin_l_and_r</item>
        <item name="android:layout_marginRight">@dimen/login_edit_margin_l_and_r</item>
        <item name="android:layout_marginEnd">@dimen/login_edit_margin_l_and_r</item>
    </style>

    <style name="CommonWavesLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/common_button_height</item>
        <item name="android:background">@drawable/selector_common_button_bg</item>
    </style>

    <style name="LoginInfoText">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/login_edit_margin_parent_l</item>
        <item name="android:textColor">@color/color_336600</item>
        <item name="android:textSize">@dimen/complete_info_textsize</item>
        <item name="android:background">@null</item>
    </style>

    <style name="LoginEdit">
        <item name="android:layout_width">@dimen/login_default_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">@dimen/login_edit_margin_parent_l</item>
        <item name="android:layout_marginStart">@dimen/login_edit_margin_parent_l</item>
        <item name="android:textColorHint">@color/color_89e00d</item>
        <item name="android:textColor">@color/color_ffffff</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">@dimen/login_edit_default_textsize</item>
    </style>
    <style name="HomeNavBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">@dimen/home_nav_bar__style_marginleft</item>
        <item name="android:textColor">@color/color_006666</item>
        <item name="android:textSize">@dimen/home_nav_bar_textsize</item>
        <item name="android:duplicateParentState">true</item>
    </style>

    <style name="CommonButton">


    </style>

    <style name="CommonQuestionFragmentTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">@dimen/question_fragment_q_margin_l_r</item>
        <item name="android:layout_marginStart">@dimen/question_fragment_q_margin_l_r</item>
        <item name="android:layout_marginTop">@dimen/question_fragment_a_margin_t</item>
        <item name="android:layout_marginBottom">@dimen/question_fragment_a_margin_t</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/choose_fragment_item_content_text_size</item>
        <item name="android:lineSpacingExtra">@dimen/question_fragment_q_line_space</item>
        <item name="android:lineSpacingMultiplier">1</item>
    </style>
    <style name="AlertDialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>
    <style name="ShadowTextStyle">
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>

    <style name="AnswerCarDialog" parent="android:style/Theme.Dialog">
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
    </style>

    <style name="AnalysisViewLeftTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/analysis_view_paddingbottom</item>
        <item name="android:layout_marginBottom">@dimen/analysis_view_paddingbottom</item>
        <item name="android:layout_marginLeft">@dimen/analysis_view_paddingbottom</item>
        <item name="android:textColor">@color/color_69ad0a</item>
        <item name="android:textSize">@dimen/analysis_titleview_textsize</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ClassifyAnalysisItemTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/analysis_view_paddingbottom</item>
        <item name="android:layout_marginBottom">@dimen/analysis_view_paddingbottom</item>
        <item name="android:textColor">@color/color_666666</item>
        <item name="android:textSize">@dimen/classify_analysis_titleview_textsize</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="CheckMobileText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingStart">@dimen/check_mobile_layout_padding</item>
        <item name="android:background">@color/color_fafafa</item>
        <item name="android:textSize">@dimen/check_mobile_textsize</item>
        <item name="android:textColorHint">@color/color_cccccc</item>
        <item name="android:textColor">@color/color_333333</item>
    </style>

    
    <style name="SettingWavesButton">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/check_mobile_wave_w</item>
        <item name="android:layout_height">@dimen/check_mobile_wave_h</item>
        <item name="android:layout_marginTop">@dimen/check_mobile_wave_margin_t</item>
    </style>
</resources>
